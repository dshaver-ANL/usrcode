      real dens,visc,thcond,thcap,beta
      real dens0,visc0,thcond0,thcap0,h_ref,T_ref
      integer ifld_h

      common /FLUIDPROP/ dens0,visc0,thcond0,thcap0,h_ref,T_ref
     & ,dens(lx1,ly1,lz1,lelv),visc(lx1,ly1,lz1,lelv)
     & ,thcap(lx1,ly1,lz1,lelv),thcond(lx1,ly1,lz1,lelv)
     & ,beta(lx1,ly1,lz1,lelv)
     & ,ifld_h
